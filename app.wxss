.social_group {
  /* visibility: hidden;  */
  display: none;
}

.social_group .list {
  transition: all 0.25s;
  transform: translateY(50rpx);
  opacity: 1;
}

.social_group .list:nth-last-of-type(1) {
  transform: translateY(130rpx);
  transition-delay: 0.1s;
}

.social_group .list:nth-last-of-type(2) {
  transform: translateY(220rpx);
  transition-delay: 0.2s;
}

.social_group .list:nth-last-of-type(3) {
  transform: translateY(310rpx);
  transition-delay: 0.3s;
}

.social_group .list:nth-last-of-type(4) {
  transform: translateY(400rpx);
  transition-delay: 0.4s;
}

.social_group .list:nth-last-of-type(5) {
  transform: translateY(490rpx);
  transition-delay: 0.5s;
}

.social_group.on {
  /* visibility: visible; */
  display: block;
}

.social_group.on .list {
  transform: translateY(0);
  opacity: 1;
}

.social_group .txt {
  width: 85rpx;
  float: left;
  color: #fdfdfd;
  margin-top: 20rpx;
}

.social_group image {
  width: 60rpx;
  height: 60rpx;
  margin: 10rpx 20rpx;
  float: left;
  padding: 5rpx;
  background: #fff;
  border-radius: 50%;
}

.social_group:after {
  content: " ";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.shade {
  width: 100%;
  height: 100%;
  transition: all 2s;
  position: fixed;
  background: #000;
  opacity: 0;
  filter: Alpha(opacity=30);
  visibility: hidden;
  z-index: 1;
}

.shade.on {
  opacity: 0.5;
  visibility: visible;
}
